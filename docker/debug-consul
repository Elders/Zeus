docker rm -f consul
docker run \
           -p 8500:8500 \
           -p 8400:8400 \
           -p 8300:8300/tcp \
           -p 8301:8301 \
           -p 8302:8302 \
           -p 8300:8300/udp \
           -p 8301:8301/udp \
           -p 8302:8302/udp \
           -p 8600:8600 \
           -p 8600:8600/udp \
           -e NODE_NAME=DOCKER-VM \
      --name consul eldersoss/consul-healthcheck:latest -ui -advertise=192.168.99.100 -retry-join=192.168.0.101 -bootstrap-expect=2